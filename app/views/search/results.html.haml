%h1.underline= t :title_search_result_for, :who => params[:q]

- if @results.empty?
  %strong= t :message_search_domain_not_found
- else
  .table-wrapper
    %table.gridwide
      %tr
        %th= t(:header_domain)
        %th= t(:header_type)
        %th= t(:header_user)
        %th.width100px= t(:header_actions)
      = render :partial => '/domains/domain', collection: @results

  = will_paginate @results